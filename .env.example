# CORS Settings
# Comma-separated list of allowed origins
CORS_ORIGINS=http://localhost:8000,https://your-production-domain.com
CORS_ALLOW_CREDENTIALS=true

# Redis Settings
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# Rate Limiting (requests per minute)
RATE_LIMIT_WEBHOOK=60
RATE_LIMIT_API=100
RATE_LIMIT_DASHBOARD=30

# Chatwoot Settings
CHATWOOT_SECRET=your_chatwoot_webhook_secret
CHATWOOT_API_URL=https://app.chatwoot.com/api/v1

# LangSmith Settings
LANGSMITH_API_KEY=your_langsmith_api_key
LANGCHAIN_PROJECT=chatwoot-ai
LANGCHAIN_TRACING_V2=true

# Cost Monitoring
DAILY_TOKEN_LIMIT=100000
MONTHLY_BUDGET=100.0
